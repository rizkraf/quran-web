  "dependencies": {
    "@vueuse/core": "^8.9.4",
    "@vueuse/head": "^0.7.6",
    "axios": "^0.27.2",
    "ts-node": "^10.9.1",
    "vue": "^3.2.36",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^20.5.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.27",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^2.9.9",
    "vue-tsc": "^1.8.8"
